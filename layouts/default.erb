    <head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
      <title><%= @item[:pagetitle] ? @item[:pagetitle] : @item[:title] %></title>
      <meta name="description" content="<%= @item[:metadescr] %>">
      <meta name="viewport" content="width=device-width">
      <%= css_link("/assets/css/main") %>
        <link rel="stylesheet" href="/assets/css/header.css">
      <link rel="stylesheet" type="text/css" media="print" href="print.css" />

      <link type="image/x-icon" href="/assets/img/nosql_favicon.ico" rel="shortcut icon">

      <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-7763794-10']);
      _gaq.push(['_setDomainName', '.couchbase.com']);
      _gaq.push(['_setAllowLinker', true]);
      _gaq.push(['_setAllowHash', false]);
      _gaq.push(['_trackPageview']);
      (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
      </script>

    </head>

<body>

    <div class="container-fluid">

        <div class="masthead">
                                <!-- if masthead height changes, make sure you also change jumptarget height in main.less-->
            <div id="header">
                  <div id="header-main">
                    <div id="header-inner">
                      <div id="logo">
                        <a title="Couchbase" href="http://www.couchbase.com/"><img width="86" height="36" alt="Couchbase" src="/assets/img/logo.png"> </a>
                      </div>
                      <div class="clear-block block block-menu_block" id="block-menu_block-12">
                        <div class="content">
                          <div class="menu-block-12 menu-name-primary-links parent-mlid-0 menu-level-1">
                            <ul class="menu">
                              <li class="leaf first menu-mlid-13118"><a title="" href="http://www.couchbase.com/why-nosql/nosql-database">Why NoSQL?</a></li>
                              <li class="leaf menu-mlid-13119 has-children"><a title="" href="http://www.couchbase.com/couchbase-server/overview">Couchbase Server</a></li>
                              <li class="leaf menu-mlid-13120"><a title="" href="http://www.couchbase.com/download">Download</a></li>
                              <li class="leaf menu-mlid-13122"><a title="" href="http://www.couchbase.com/learn">Resources</a></li>
                              <li class="leaf menu-mlid-16593"><a title="" href="http://www.couchbase.com/communities/">Communities</a></li>
                              <li class="leaf last menu-mlid-13123"><a title="" href="http://www.couchbase.com/customer-stories">Customers</a></li>
                            </ul>
                          </div>
                        </div>
                      </div>
                      <div class="clear-block block block-google_cse" id="block-google_cse-0">
                        <gcse:searchbox></gcse:searchbox>
                      </div>

                    </div>
                  </div>
                </div>


                <div id="header-nav">
                               <div id="cse-search" class="navbar-search pull-right">


                               <p class="guide_title"><%= section_name %></p>

                          <div class="version pull-right">

                          <%= render 'version_widget' %></div>


                        <!--<ul class = "nav nav-pills pull-right">

                            <li><a href="#">hello</a></li>
                              <li><a href="#">world</a></li>
                            </ul> -->


                    </div>

                            <ul class="nav nav-pills pull-left">
                        <li>
                            <a href="/index.html"><%= image_tag("/assets/img/home_icon") %></a>
                        </li>
                        <%= render 'navbar_widget' %>
                        <li>
                            <a href="/prebuilt/translations/jp/index-ja.html"><%= image_tag("/assets/img/ja") %></a>
                        </li>
                        <li>
                            <a href="http://www.couchbase.com/kr"><%= image_tag("/assets/img/ko") %></a>
                        </li>
                    </ul>

                </div>

                     <div id="cse-results">
                                <gcse:searchresults></gcse:searchresults>
                            </div>
   </div>

        <div class="row-fluid">
            <% if @item[:ng] %>
                <% children = (@item[:index] ? @item.children : @item.parent.children).sort_by(&:identifier) %>
                <div id="outernav" class="span3 hidden-phone">
                    <ul id="sidenav" class="span3 nav nav-list affix">
                        <li><h3><%= section_name %></h3></li>
                        <% children.each do |entry| %>
                            <li class="<%= (entry.identifier == @item.identifier) ? 'active' : '' %>">
                                <%= link_to entry[:title], entry %>
                            </li>
                        <% end %>
                    </ul>
                </div>
            <% elsif !@item[:no_toc] %>
                <%= render 'toc_widget' %>
            <% end %>

            <div class="<%= @item[:no_toc] ? "span12" : "span9" %>  mdcontent <%= @item[:css_class] %>">
                <%= yield %>
            </div>
        </div>

      </div>

    </div>


        <div class="row-fluid">
            <% unless @item[:no_toc] %>
                <%= render 'toc_widget' %>
                <% end %>

					<% if @item[:no_toc] %>
						<div class="span12 mdcontent <%= @item[:css_class] %>">
					<% else %>
						<div class="span9 mdcontent <%= @item[:css_class] %>">
					<% end %>

                <%= yield %>
            </div>
        </div>

    <%= js_link("/assets/js/main") %>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script>
      (function() {
        var cx = '018016427239405524608:fkg1v30apnm';
        var gcse = document.createElement('script');
        gcse.type = 'text/javascript';
        gcse.async = true;
        gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
            '//www.google.com/cse/cse.js?cx=' + cx;
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(gcse, s);
      })();
    </script>
  </body>
</html>
